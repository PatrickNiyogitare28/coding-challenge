{
  "name": "coding-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run",
    "g:migration": "yarn typeorm:local migration:generate -n",
    "g:entity": "yarn typeorm:local entity:create -n",
    "db:setup": "yarn db:create:dev && yarn db:create:test",
    "db:create:dev": "createdb --owner=postgres next_now_dev",
    "db:create:test": "createdb --owner=postgres next_now_test",
    "db:drop:local": "yarn typeorm:local schema:drop",
    "db:drop:dev": "yarn typeorm:dev schema:drop",
    "db:drop:staging": "yarn typeorm:staging schema:drop",
    "db:drop:prod": "yarn typeorm:prod schema:drop",
    "db:migrate:local": "yarn typeorm:local migration:run",
    "db:migrate:local:testing": "yarn typeorm:local:testing migration:run",
    "db:migrate:dev": "yarn typeorm:dev migration:run",
    "db:migrate:staging": "yarn typeorm:staging migration:run",
    "db:migrate:prod": "yarn typeorm:prod migration:run",
    "db:revert:local": "yarn typeorm:local migration:revert",
    "db:revert:local:testing": "yarn typeorm:local:testing migration:revert",
    "db:revert:dev": "yarn typeorm:dev migration:revert",
    "db:revert:staging": "yarn typeorm:staging migration:revert",
    "db:revert:prod": "yarn typeorm:prod migration:revert",
    "db:seed": "yarn hygen seed run",
    "db:seeds:up": "yarn local seeds/local/seedsUp.ts",
    "typeorm:local": "yarn local ./node_modules/typeorm/cli.js",
    "typeorm:local:testing": "yarn local:testing ./node_modules/typeorm/cli.js",
    "typeorm:dev": "yarn development ./node_modules/typeorm/cli.js",
    "typeorm:staging": "yarn staging ./node_modules/typeorm/cli.js",
    "typeorm:prod": "yarn production ./node_modules/typeorm/cli.js",
    "local": "DOTENV_CONFIG_PATH=./.env ts-node -P ./tsconfig.yarn.json -r dotenv/config",
    "local:testing": "DOTENV_CONFIG_PATH=./.env.test ts-node -P ./tsconfig.yarn.json -r dotenv/config",
    "development": "DOTENV_CONFIG_PATH=.env.development ts-node -P ./tsconfig.yarn.json -r dotenv/config",
    "staging": "DOTENV_CONFIG_PATH=.env.staging ts-node -P ./tsconfig.yarn.json -r dotenv/config ",
    "production": "DOTENV_CONFIG_PATH=.env.prod ts-node -P ./tsconfig.yarn.json -r dotenv/config",
    "deploy:staging": "now -A staging.now.json --target=staging --scope=mthomps4 && now alias -A staging.now.json",
    "deploy:prod": "now -A prod.now.json --target=production --scope=mthomps4 && now alias -A prod.now.json",
    "ci:deploy:staging": "now -A staging.now.json --target=staging --token=$NOW_TOKEN --scope=mthomps4  && now alias -A staging.now.json  --token=$NOW_TOKEN",
    "ci:deploy:prod": "now -A prod.now.json --target=production --token=$NOW_TOKEN --scope=mthomps4 && now alias -A prod.now.json  --token=$NOW_TOKEN"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "formik": "^2.2.9",
    "har-validator": "^5.1.5",
    "hygen": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^7.0.1",
    "now": "^21.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.1.1",
    "react-redux": "^7.2.4",
    "sass": "^1.32.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "autoprefixer": "^10.1.0",
    "cypress": "^6.2.1",
    "dotenv": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "postcss": "^8.2.4",
    "prisma": "^3.6.0",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
